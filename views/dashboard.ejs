                    <div class="col-md-12">
                      <input class="form_control" placeholder="Name" type="text" id="photoname" name="photoname">
                    </div>
                    <div class="col-md-12">
                      <textarea name="description" style="height: 150px;" class="form_control" id="description" placeholder="Description"
                        rows="5"></textarea>
                    </div>
                    <div style="margin-bottom: 20px ;" class="col-md-12">
                      <input type="file" name="image" class="form-control-file">
                    </div>
                    <div class="col-md-12">
                      <div class="group_form">
                        <input  class="read_more" type="submit" value="Upload File">
                      </div>
                    </div>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
